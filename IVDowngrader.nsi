Function .onInit
    SectionSetSize ${section_downgrade} 18073792
    SectionSetSize ${section_patch} 800000
    SectionSetFlags ${section_downgrade} 17

    ; Get current CE install folder from registry
    ${If} ${RunningX64}
        SetRegView 64
        ReadRegStr $0 HKLM "SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\Steam App 12210" "InstallLocation"
        SetRegView LastUsed
    ${Else}
        ReadRegStr $0 HKLM "SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\Steam App 12210" "InstallLocation"
    ${EndIf}
    
    ; If CE install path not found, use default one
    ${IfNot} $0 == ""
        StrCpy $CE_FOLDER "$0\GTAIV"
    ${Else}
        StrCpy $CE_FOLDER "$PROGRAMFILES\Steam\steamapps\common\Grand Theft Auto IV\GTAIV"
    ${EndIf}

    StrCpy $SelectedPatch "1.0.4.0 (Patch 4)"
    
FunctionEnd

Function .onGUIEnd
    Linker::unload
FunctionEnd


