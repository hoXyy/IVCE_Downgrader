Function patchSelectionEnter
    ; Only show this page if patch section is enabled
    SectionGetFlags ${section_patch} $0
    IntOp $0 $0 & ${SF_SELECTED}
    IntCmp $0 ${SF_SELECTED} showPage

    Abort

    showPage: 
        !insertmacro MUI_HEADER_TEXT "Patch selection" "Select which patch you would like to install after the downgrade is complete."
        nsDialogs::Create 1018
        Pop $PatchPageDialog

        ${If} $PatchPageDialog == error
            Abort
        ${EndIf}

        ; Create instruction label
        ${NSD_CreateLabel} 0 0 100% 12u "Select which patch to install from the dropdown list down below:"
        Pop $PatchPageTopText

        ; Create dropdown
        ${NSD_CreateDropList} 0 20 100% 12u "Select patch to install..."
        Pop $PatchPageDropList

        ; Add all patches to dropdown    
        ${NSD_CB_AddString} $PatchPageDropList "1.0.6.2 (Patch 8 (Japanese))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.7.1 (Patch 8 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.8.0 (Patch 8)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.5.2 (Patch 7 (Japanese))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.6.1 (Patch 7 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.7.0 (Patch 7)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.4.2 (Patch 6 (Japanese))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.5.1 (Patch 6 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.6.0 (Patch 6)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.0.4 (Patch 5)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.3.1 (Patch 4 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.4.0 (Patch 4)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.2.2 (Patch 3 (Japanese))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.2.1 (Patch 3 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.3.0 (Patch 3)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.1.2 (Patch 2 (Japanese))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.1.1 (Patch 2 (Russian))"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.2.0 (Patch 2)"
        ${NSD_CB_AddString} $PatchPageDropList "1.0.1.0 (Patch 1)"

        ; Add onChange handler
        ${NSD_OnChange} $PatchPageDropList onPatchChange

        ; Create autosplitter note
        ${NSD_CreateLabel} 0 100 100% 20u "WARNING: Not all patches are supported by the autosplitter! Refer to the link below for the list of patches that the autosplitter supports:"
        Pop $PatchPageNoteText
        CreateFont $1 "Tahoma" 8 700
        SendMessage $PatchPageNoteText ${WM_SETFONT} $1 0

        ; Add autosplitter link
        ${NSD_CreateLabel} 0 130 100% 12u "https://github.com/jfoster/LiveSplit.ASL/tree/dev/GTAIV"
        Pop $PatchPageAutosplitterURL
        SendMessage $PatchPageAutosplitterURL ${WM_SETFONT} $1 0
        Linker::link /NOUNLOAD $PatchPageAutosplitterURL "https://github.com/jfoster/LiveSplit.ASL/tree/dev/GTAIV"

        ; Create patch note
        ${NSD_CreateLabel} 0 160 100% 20u "NOTE: Not all of the available patches here have been fully tested in-game! The Japanese/Russian patches are included mostly for experimental purposes."
        Pop $PatchPagePatchText

        ; Store next button variable and disable it if combobox is empty (need to do it if user comes back to this page)
        GetDlgItem $PatchPageNextButton $HWNDPARENT 1
        ${If} $SelectedPatch == "" 
            EnableWindow $PatchPageNextButton 0
        ${Else}
            EnableWindow $PatchPageNextButton 1
        ${EndIf}

        ; Set combobox to stored value if user came back to the page
        ${NSD_CB_SelectString} $PatchPageDropList "$SelectedPatch"

        nsDialogs::Show
FunctionEnd

Function onPatchChange
    Pop $0
    SendMessage $0 ${CB_GETCURSEL} "" "" $2
    System::Call 'USER32::SendMessage(i$0,i${CB_GETLBTEXT},i$2,t.r2)'
    ${If} $2 == ""
        EnableWindow $PatchPageNextButton 0
    ${Else}
        EnableWindow $PatchPageNextButton 1
        StrCpy $SelectedPatch $2
    ${EndIf}
FunctionEnd

Function LocateCallback
  ; Make sure details print
  SetDetailsPrint both

  ; Create the directory
  ; ($R6 is blank if file)
  StrCmp $R6 "" 0 copyfiles
  CreateDirectory "$R9"
  IfErrors FolderError end
  
  copyfiles:  
  push $R0
  ${WordFind} "$R9" "$CE_FOLDER" -1 $R0
  CopyFiles /SILENT "$R9" "$INSTDIR\Grand Theft Auto IV$R0"
  pop $R0
  IfErrors FileError 0
  goto end
  
  FolderError:
  DetailPrint "ERROR CREATING FOLDER $R7!"
  goto end
  
  FileError:
  DetailPrint "ERROR COPYING FILE $R7!"
  
  end:
  StrCpy $ErrorFlag -1
  StrCmp $R9 "" 0 +2 ; no more files
  StrCpy $0 StopLocate
  Push $0
FunctionEnd