Section "Install a patch for GTA IV" section_patch
    SetOutPath "$INSTDIR\Patch"

    DetailPrint "Running patch install..."
    ${SetTitleBar} "Running patch install..."

    ${Select} $SelectedPatch
        ${Case} "1.0.1.0 (Patch 1)"
            File "TitleUpdates\1.0.1.0\GTAIV_Patch.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_Patch.exe"
        ${Case} "1.0.2.0 (Patch 2)"
            File "TitleUpdates\1.0.2.0\GTAIV_Patch_V2.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_Patch_V2.exe"
        ${Case} "1.0.1.1 (Patch 2 (Russian))"
            File "TitleUpdates\1.0.2.0\GTAIV_PATCH_V1_RUS.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_PATCH_V1_RUS.exe"
        ${Case} "1.0.1.2 (Patch 2 (Japanese))"
            File "TitleUpdates\1.0.2.0\GTAIV_Patch_1012.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_Patch_1012.exe"
        ${Case} "1.0.3.0 (Patch 3)"
            File "TitleUpdates\1.0.3.0\GTAIV_Patch_1030.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_Patch_1030.exe"
        ${Case} "1.0.2.1 (Patch 3 (Russian))"
            File "TitleUpdates\1.0.3.0\GTAIV_Patch_1021.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_Patch_1021.exe"
        ${Case} "1.0.2.2 (Patch 3 (Japanese))"
            File "TitleUpdates\1.0.3.0\GTAIV_MAINTENANCE_UPDATE_1022_JP.exe"
            ExecWait "$INSTDIR\Patch\GTAIV_MAINTENANCE_UPDATE_1022_JP.exe"
